<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane prefHeight="800.0" prefWidth="1130.0" styleClass="background-pane" stylesheets="@MagnumAppStyle.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.unil.magnumapp.view.RootLayoutController">
   <center>
      <HBox BorderPane.alignment="TOP_LEFT">
         <children>
            <VBox fx:id="leftSide" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="5.0" />
               </HBox.margin></VBox>
            <VBox fx:id="rightSide" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets bottom="10.0" left="5.0" right="10.0" />
               </HBox.margin></VBox>
         </children>
         <BorderPane.margin>
            <Insets top="2.0" />
         </BorderPane.margin>
      </HBox>
   </center>
   <top>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="NEVER" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <HBox spacing="10.0">
               <children>
                  <Button fx:id="helpButton" mnemonicParsing="false" onAction="#handleHelpButton" prefWidth="68.0" text="Help" />
                  <Button fx:id="preferencesButton" mnemonicParsing="false" onAction="#handlePreferencesButton" text="Settings" />
                  <Button fx:id="aboutButton" mnemonicParsing="false" onAction="#handleAboutButton" prefWidth="68.0" text="About" />
               </children>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets left="10.0" top="2.0" />
         </BorderPane.margin>
      </GridPane>
   </top>
</BorderPane>
